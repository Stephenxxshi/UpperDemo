<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:views="clr-namespace:Plant01.Upper.Wpf.Core.Views"
    xmlns:vm="clr-namespace:Plant01.Upper.Presentation.Core.ViewModels;assembly=Plant01.Upper.Presentation.Core">
    <DataTemplate DataType="{x:Type vm:DashboardViewModel}">
        <views:Dashboard />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:SideBarViewModel}">
        <views:SideBar />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:SettingsViewModel}">
        <views:Settings />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:ProduceRecordViewModel}">
        <views:ProduceRecord />
    </DataTemplate>
</ResourceDictionary>