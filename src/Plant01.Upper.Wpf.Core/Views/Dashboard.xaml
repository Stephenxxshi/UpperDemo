<UserControl
    x:Class="Plant01.Upper.Wpf.Core.Views.Dashboard"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:controls="clr-namespace:Plant01.WpfUI.Controls;assembly=Plant01.WpfUI"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:Plant01.Upper.Wpf.Core.Views"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:themes="clr-namespace:Plant01.WpfUI.Themes;assembly=Plant01.WpfUI"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <controls:TransitioningContentControl RunTransitionOnLoaded="True" TransitionMode="SlideLeft">
        <Grid>
            <Button Style="{StaticResource AntDesign.Button.Primary}">
                Click Me
            </Button>
        </Grid>
    </controls:TransitioningContentControl>
</UserControl>
