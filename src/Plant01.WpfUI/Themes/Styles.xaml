    <Style x:Key="TabControl" TargetType="TabControl">
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="BorderThickness" Value="0" />
        <Setter Property="Padding" Value="10,0,10,10" />
        <Setter Property="SelectedIndex" Value="0" />
        <Setter Property="ItemContainerStyle">
            <Setter.Value>
                <Style TargetType="TabItem">
                    <Setter Property="IsTabStop" Value="False" />
                    <Setter Property="Height" Value="50" />
                    <Setter Property="Width" Value="120" />
                    <Setter Property="Background" Value="Transparent" />
                    <Setter Property="FocusVisualStyle" Value="{x:Null}" />
                    <Setter Property="HorizontalAlignment" Value="Right" />
                    <Setter Property="VerticalAlignment" Value="Center" />
                    <Setter Property="Template">
                        <Setter.Value>
                            <ControlTemplate TargetType="TabItem">
                                <Grid
                                    x:Name="WrapPanel"
                                    Width="112"
                                    HorizontalAlignment="{TemplateBinding HorizontalAlignment}"
                                    VerticalAlignment="{TemplateBinding VerticalAlignment}">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="37" />
                                        <RowDefinition Height="3" />
                                    </Grid.RowDefinitions>
                                    <Grid Grid.Row="0">
                                        <Border x:Name="Header" Width="{TemplateBinding Width}" Background="{TemplateBinding Background}">
                                            <TextBlock
                                                x:Name="HeaderTxt"
                                                Margin="-8,0,0,0"
                                                Style="{StaticResource PrimaryTitle}"
                                                Text="{TemplateBinding Header}" />
                                        </Border>
                                    </Grid>
                                    <Grid Grid.Row="1">
                                        <Border
                                            x:Name="Footer"
                                            Width="0"
                                            HorizontalAlignment="Left"
                                            Background="{StaticResource Color_1}" />
                                    </Grid>
                                </Grid>
                                <ControlTemplate.Resources>
                                    <Storyboard x:Key="ActivatedStoryboard">
                                        <DoubleAnimation
                                            Storyboard.TargetName="Footer"
                                            Storyboard.TargetProperty="Width"
                                            From="0"
                                            To="120"
                                            Duration="0:0:0.3" />
                                    </Storyboard>
                                    <Storyboard x:Key="NotActiveStoryboard">
                                        <DoubleAnimation
                                            Storyboard.TargetName="Footer"
                                            Storyboard.TargetProperty="Width"
                                            From="120"
                                            To="0"
                                            Duration="0:0:0.3" />
                                    </Storyboard>
                                </ControlTemplate.Resources>
                                <ControlTemplate.Triggers>
                                    <MultiTrigger>
                                        <MultiTrigger.Conditions>
                                            <Condition SourceName="WrapPanel" Property="IsMouseOver" Value="True" />
                                            <Condition Property="TabItem.IsSelected" Value="False" />
                                        </MultiTrigger.Conditions>
                                        <MultiTrigger.EnterActions>
                                            <BeginStoryboard Storyboard="{StaticResource ActivatedStoryboard}" />
                                        </MultiTrigger.EnterActions>
                                        <MultiTrigger.ExitActions>
                                            <BeginStoryboard x:Name="NotActiveAction" Storyboard="{StaticResource NotActiveStoryboard}" />
                                        </MultiTrigger.ExitActions>
                                        <Setter TargetName="WrapPanel" Property="Cursor" Value="Hand" />
                                        <Setter TargetName="Header" Property="Background" Value="{StaticResource Color_4}" />
                                        <Setter TargetName="HeaderTxt" Property="Foreground" Value="{StaticResource Color_3}" />
                                        <Setter TargetName="HeaderTxt" Property="FontWeight" Value="SemiBold" />
                                    </MultiTrigger>
                                    <Trigger Property="TabItem.IsSelected" Value="True">
                                        <Trigger.EnterActions>
                                            <RemoveStoryboard BeginStoryboardName="NotActiveAction" />
                                        </Trigger.EnterActions>
                                        <Setter TargetName="WrapPanel" Property="Cursor" Value="Hand" />
                                        <Setter TargetName="Header" Property="Background" Value="{StaticResource Color_4}" />
                                        <Setter TargetName="HeaderTxt" Property="Foreground" Value="{StaticResource Color_3}" />
                                        <Setter TargetName="HeaderTxt" Property="FontWeight" Value="SemiBold" />
                                        <Setter TargetName="Footer" Property="Width" Value="120" />
                                    </Trigger>
                                </ControlTemplate.Triggers>
                            </ControlTemplate>
                        </Setter.Value>
                    </Setter>
                </Style>
            </Setter.Value>
        </Setter>
    </Style>


